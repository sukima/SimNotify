- confirm_on_exit
- semantic_form_for @special_event do |f|
  - f.inputs do
    = f.input :title
    = f.input :event_type, :as => :select, :collection => SpecialEvent.type_list, :include_blank => false
    = f.input :notes
    = f.input :start_time
    = f.input :end_time
    = f.input :all_day
  = f.buttons
